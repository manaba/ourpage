<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.tammy.ourpage.dao.RegisterDao">

	<insert id="insert" parameterType="com.tammy.ourpage.model.User" >
		insert into user(name,password) values(#{name},#{password})
	</insert>
	
	<select id="select" resultType="com.tammy.ourpage.model.User" >
		select * from  user
	</select>
	
</mapper>